<div class="w3-card-4">
    <div class="w3-container w3-blue">
        <h2>Add Resturant</h2>
    </div>
    <form class="w3-container" action="/insert/resturant" method="POST" enctype="multipart/form-data">
        <label>Name</label>
        <input class="w3-input" type="text" name="longName">
        <label>ShortName</label>
        <input class="w3-input" type="text" name="shortName">
        <label>Image</label>
        <input class="w3-input" type="file" name="img">
        <button class="w3-btn w3-blue" type="submit">Submit</button>
    </form>
</div>

    <div class="w3-card-4">
    <div class="w3-container w3-blue">
        <h2>Add Person</h2>
    </div>
        <form class="w3-container" action="/insert/person" method="POST" enctype="multipart/form-data">
        <label>Name</label>
        <input class="w3-input" type="text" name="Name">
        <label>Place (Shortname)</label>
        <input class="w3-input" type="text" name="place">
        <label>Venmo</label>
        <input class="w3-input" type="text" name="venmo">
        <label>Paypal</label>
        <input class="w3-input" type="text" name="paypal">
        <label>CashApp</label>
        <input class="w3-input" type="text" name="cashapp">
        <button class="w3-btn w3-blue" type="submit">Submit</button>
    </form>
</div>